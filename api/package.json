{
  "name": "villager-api",
  "version": "1.0.0",
  "description": "api for villager.io",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
