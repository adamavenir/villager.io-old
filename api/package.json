  "dependencies": {
    "bcrypt": "^0.8.5",
    "boom": "^3.1.2",
    "getconfig": "^2.2.0",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "hapi": "^13.2.1",
    "hapi-auth-jwt": "^4.0.0",
    "joi": "^8.0.4",
